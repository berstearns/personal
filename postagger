openapi: 3.0.0
info:
  title: pos tagger from sentence
  description: Tree tagger
  version: 1.0.0-oas3
paths:
  '/sentences_tagger':
    post:
      operationId: postagger   
      endpoint: postagger   
      summary: Returns a part of speech for given sentences on a blob file
      requestBody:
        content:
          application/json:
            schema:
              properties:
                sentences:
                  collectionFormat: multi
                  items:
                    type: string
                  type: array
        '200':
          description: A formatted array of sentences of pos tags
          content:
            application/json:
              schema:
                  type: string
